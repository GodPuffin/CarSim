<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.carsim.SimController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="400.0" spacing="7.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="JavaFX Car Simulation" textAlignment="CENTER">
               <fill>
                  <LinearGradient cycleMethod="REPEAT" endX="1.0" endY="1.0">
                     <stops>
                        <Stop>
                           <color>
                              <Color red="1.0" green="0.39629629254341125" blue="0.8591358065605164" />
                           </color>
                        </Stop>
                        <Stop offset="1.0">
                           <color>
                              <Color red="0.3740740716457367" green="0.8226543068885803" blue="1.0" />
                           </color>
                        </Stop>
                     </stops>
                  </LinearGradient>
               </fill>
               <font>
                  <Font size="30.0" />
               </font>
            </Text>
            <Separator prefWidth="200.0" />
            <GridPane alignment="CENTER" hgap="5.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="286.99998474121094" minWidth="10.0" prefWidth="260.99998474121094" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.99999999999997" minWidth="10.0" prefWidth="107.00001525878906" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Slider majorTickUnit="50.0" showTickLabels="true" showTickMarks="true" />
                  <Slider majorTickUnit="50.0" showTickLabels="true" showTickMarks="true" GridPane.rowIndex="1" />
                  <Slider fx:id="scaleSlider" majorTickUnit="50.0" max="15.0" min="1.0" showTickLabels="true" showTickMarks="true" value="5.0" GridPane.rowIndex="2" />
                  <Label text=" Car Weight" GridPane.columnIndex="1" />
                  <Label text=" Engine Force" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text=" Zoom Amount" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER" prefHeight="75.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Surface:" />
                  <ComboBox fx:id="surfaceComboBox" onAction="#SurfaceSelected" prefWidth="150.0" value="Asphalt">
                      <items>
                          <FXCollections fx:factory="observableArrayList">
                              <String fx:value="Asphalt" />
                              <String fx:value="Gravel" />
                              <String fx:value="Ice" />
                          </FXCollections>
                      </items>
                  </ComboBox>
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <Button fx:id="resetButton" mnemonicParsing="false" onAction="#resetPressed" text="Reset" />
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitPressed" text="Exit" />
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <GridPane alignment="CENTER" hgap="5.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="405.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <LineChart createSymbols="false" legendVisible="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" title="Acceleration X">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <LineChart createSymbols="false" legendVisible="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" title="Acceleration Y" GridPane.columnIndex="1">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <LineChart createSymbols="false" legendVisible="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" title="Velocity X" GridPane.rowIndex="1">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <LineChart createSymbols="false" legendVisible="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" title="Velocity Y" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <LineChart createSymbols="false" legendVisible="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0" title="Speed" GridPane.rowIndex="2">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <children>
                        <Label text="Throttle %" />
                        <ProgressBar fx:id="throttleBar" prefWidth="190.0" progress="0.0">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </ProgressBar>
                        <Label text="RPM" />
                        <ProgressBar prefWidth="190.0" progress="0.0">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </ProgressBar>
                        <Label text="Torque" />
                        <ProgressBar prefWidth="190.0" progress="0.0" />
                     </children>
                  </VBox>
               </children>
            </GridPane>
         </children>
      </VBox>
      <Pane fx:id="mainCanvas" />
<!--height="900.0" width="1200.0"-->
   </children>
   <padding>
      <Insets left="10.0" top="10.0" />
   </padding>
</HBox>
